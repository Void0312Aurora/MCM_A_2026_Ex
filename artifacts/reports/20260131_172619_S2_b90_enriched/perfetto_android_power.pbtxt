duration_ms: 540000
buffers: {
  size_kb: 2048
  fill_policy: RING_BUFFER
}
data_sources: {
  config {
    name: "android.power"
    android_power_config {
      battery_poll_ms: 250
      counters: BATTERY_COUNTER_CAPACITY_PERCENT
      counters: BATTERY_COUNTER_CHARGE
      counters: BATTERY_COUNTER_CURRENT
      counters: BATTERY_COUNTER_VOLTAGE
    }
  }
}
